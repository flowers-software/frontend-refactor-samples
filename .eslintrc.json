{ 
    "env":{ 
       "browser":true,
       "es6":true
    },
    "extends":[ 
       "google"
    ],
    "globals":{ 
       "Atomics":"readonly",
       "SharedArrayBuffer":"readonly"
    },
    "parserOptions":{ 
       "ecmaFeatures":{ 
          "jsx":true
       },
       "ecmaVersion":2018,
       "sourceType":"module"
    },
    "plugins":[ 
       "react"
    ],
    "rules":{ 
       "indent": [ 1, 4 ],
       "max-len": [ 1, { "code": 200 } ],
       "linebreak-style": "off",
       "react/jsx-uses-react": 1,
       "react/jsx-uses-vars": 1,
       "require-jsdoc": "off"
    }
 }